<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CreateConferenceActivity">


    <ImageView
        android:layout_width="269dp"
        android:layout_height="307dp"
        android:background="@drawable/secretary_kim"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="50dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="회의제목을 입력하세요."
        android:textColor="#000000"
        android:textSize="30dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="392dp"></TextView>

    <EditText
        android:id="@+id/EditText_conference"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="432dp"></EditText>

    <Button
        android:id="@+id/Button_create"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:background="@drawable/custom_button2"
        android:text="회의생성"
        android:textColor="#FFFFFF"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="627dp"></Button>

</androidx.constraintlayout.widget.ConstraintLayout>